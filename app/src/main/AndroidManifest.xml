<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="dk.brams.android.drawerexample">


    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activities.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="dk.brams.android.drawerexample.Activities.AboutUsActivity"
            android:label="@string/activity_title_about_us" />
        <activity
            android:name="dk.brams.android.drawerexample.Activities.PrivacyPolicyActivity"
            android:label="@string/activity_title_privacy_policy" />
    </application>

</manifest>